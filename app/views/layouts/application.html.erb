<!DOCTYPE html>
<html  ng-app='Ekart'>
  <head>
    <title>Ekart</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= csrf_meta_tags %>
  </head>
  <body ng-controller='ItemController'>
    <div class='row'>
      <%= render 'layouts/header' %>
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>
  </body>
  <%= javascript_include_tag 'application' %>
</html>
